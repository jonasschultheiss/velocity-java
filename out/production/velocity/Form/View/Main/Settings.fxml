<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0"
            prefWidth="1080.0" style="-fx-background-color: #303030;" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <Label layoutX="56.0" layoutY="46.0" text="Settings" textFill="WHITE" AnchorPane.leftAnchor="60.0"
           AnchorPane.topAnchor="50.0">
        <font>
            <Font name="DINPro-Bold" size="50.0"/>
        </font>
    </Label>
    <Label layoutX="60.0" layoutY="180.0" text="Set profile picture:" textFill="WHITE" AnchorPane.leftAnchor="60.0">
        <font>
            <Font name="DINPro-Regular" size="13.0"/>
        </font>
    </Label>
    <Pane layoutX="60.0" layoutY="199.0" prefHeight="200.0" prefWidth="253.0" style="-fx-background-color: #353535;"
          AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="200.0">
        <padding>
            <Insets bottom="50.0"/>
        </padding>
        <Button fx:id="btnPP" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#choosePP"
                onMouseEntered="#enterBtn" onMouseExited="#exitBtn" prefHeight="28.0" prefWidth="156.0"
                style="-fx-background-color: #252525; -fx-text-fill: white; -fx-font-family: DINPro-Regular;"
                text="Choose a profile picture"/>
        <Label fx:id="lblImageName" layoutX="14.0" layoutY="73.0" style="-fx-text-fill: white;" text="Label">
            <font>
                <Font name="DINPro-Regular" size="13.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="112.0" style="-fx-text-fill: white;"
               text="You need to restart velocity&#10;in order to see your new profile&#10;picture."/>
    </Pane>
    <Pane layoutX="60.0" layoutY="444.0" prefHeight="160.0" prefWidth="253.0" style="-fx-background-color: #353535;"
          AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="60.0">
        <padding>
            <Insets bottom="50.0"/>
        </padding>
        <Button layoutX="14.0" layoutY="118.0" mnemonicParsing="false" onAction="#resetTeam" onMouseEntered="#enterBtn"
                onMouseExited="#exitBtn" prefHeight="27.0" prefWidth="114.0"
                style="-fx-background-color: #252525; -fx-text-fill: white;" text="Reset your team">
            <font>
                <Font name="DINPro-Regular" size="13.0"/>
            </font>
        </Button>
        <Label layoutX="14.0" layoutY="14.0" style="-fx-text-fill: white;"
               text="Your team will get newly loaded and&#10;thus reseted.&#10;Only choose this option if you've&#10;encountered a bug."/>
    </Pane>
    <Label layoutX="60.0" layoutY="439.0" text="Reset team:" textFill="WHITE">
        <font>
            <Font name="DINPro-Regular" size="13.0"/>
        </font>
    </Label>
    <Pane layoutX="416.0" layoutY="199.0" prefHeight="433.0" prefWidth="253.0" style="-fx-background-color: #353535;"
          AnchorPane.bottomAnchor="100.0" AnchorPane.topAnchor="200.0">
        <padding>
            <Insets bottom="50.0"/>
        </padding>
        <Button layoutX="16.0" layoutY="377.0" mnemonicParsing="false" onAction="#changeUsername"
                onMouseEntered="#enterBtn" onMouseExited="#exitBtn" prefHeight="27.0" prefWidth="225.0"
                style="-fx-background-color: #252525; -fx-text-fill: white; -fx-font-family: DINPro-Regular;"
                text="Change username">
            <font>
                <Font name="DINPro-Regular" size="13.0"/>
            </font>
        </Button>
        <TextField fx:id="txfCurrentUsername" layoutX="14.0" layoutY="40.0" prefHeight="27.0" prefWidth="225.0"
                   style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <Label layoutX="14.0" layoutY="23.0" text="Current username:" textFill="WHITE"/>
        <TextField fx:id="txfNewUsername" layoutX="14.0" layoutY="99.0" prefHeight="27.0" prefWidth="225.0"
                   style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <Label layoutX="14.0" layoutY="82.0" text="New username:" textFill="WHITE"/>
        <Label layoutX="14.0" layoutY="141.0" text="Confirm new username:" textFill="WHITE"/>
        <TextField fx:id="txfConfNewUsername" layoutX="14.0" layoutY="158.0" prefHeight="27.0" prefWidth="225.0"
                   style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <Label fx:id="lblErrorUsername" layoutX="16.0" layoutY="210.0" text="\${error_message}" textFill="RED"
               visible="false"/>
    </Pane>
    <Label layoutX="416.0" layoutY="180.0" text="Change username:" textFill="WHITE">
        <font>
            <Font name="DINPro-Regular" size="13.0"/>
        </font>
    </Label>
    <Pane layoutX="766.0" layoutY="199.0" prefHeight="460.0" prefWidth="253.0" style="-fx-background-color: #353535;"
          AnchorPane.bottomAnchor="100.0" AnchorPane.rightAnchor="60.0" AnchorPane.topAnchor="200.0">
        <padding>
            <Insets bottom="50.0"/>
        </padding>
        <Label layoutX="17.0" layoutY="140.0" text="Confirm new password:" textFill="WHITE"/>
        <Label layoutX="17.0" layoutY="81.0" text="New password:" textFill="WHITE"/>
        <Label layoutX="17.0" layoutY="22.0" text="Current password:" textFill="WHITE"/>
        <Button layoutX="15.0" layoutY="380.0" mnemonicParsing="false" onAction="#changePassword"
                onMouseEntered="#enterBtn" onMouseExited="#exitBtn" prefHeight="27.0" prefWidth="225.0"
                style="-fx-background-color: #252525; -fx-text-fill: white; -fx-font-family: DINPro-Regular;"
                text="Change password">
            <font>
                <Font name="DINPro-Regular" size="13.0"/>
            </font>
        </Button>
        <PasswordField fx:id="txfCurrentPassword" layoutX="17.0" layoutY="39.0" prefHeight="27.0" prefWidth="221.0"
                       style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <PasswordField fx:id="txfNewPassword" layoutX="17.0" layoutY="98.0" prefHeight="27.0" prefWidth="221.0"
                       style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <PasswordField fx:id="txfConfNewPassword" layoutX="17.0" layoutY="157.0" prefHeight="27.0" prefWidth="221.0"
                       style="-fx-background-color: #252525; -fx-text-fill: white;"/>
        <Label fx:id="lblErrorPassword" layoutX="19.0" layoutY="210.0" text="\${error_message}" textFill="RED"
               visible="false"/>
    </Pane>
    <Label layoutX="768.0" layoutY="180.0" text="Change password:" textFill="WHITE">
        <font>
            <Font name="DINPro-Regular" size="13.0"/>
        </font>
    </Label>
    <Button fx:id="btnSave" layoutX="767.0" layoutY="648.0" mnemonicParsing="false" onAction="#saveAllChanges"
            onMouseEntered="#enterBtn" onMouseExited="#exitBtn" prefHeight="37.0" prefWidth="253.0"
            style="-fx-background-color: #252525; -fx-text-fill: white; -fx-font-family: DINPro-Regular;"
            text="Save all changes."/>
    <Label fx:id="lblErrorUnsaved" layoutX="780.0" layoutY="631.0" text="Unsaved changes" textFill="RED"
           visible="false"/>
</AnchorPane>
